<%- include("./partials/head")%>

    <link rel="stylesheet" href="stylesheets/cadastro-pessoa.css">



    <title> Cadastre-se - Monstro Suplementos</title>
    </head>

    <body>
        <main>
            <header>
                <img src="/images/logo.oficial.jpeg" alt="">
            </header>

            <h1 id="titulo">Seja Um Parceiro! #Vemmostro</h1>

            <div class="forms">
                <input id="nomec" type="text" placeholder=" Nome Completo">
                <input id="nomes" type="text" placeholder=" Nome Social">
                <input id="cpf" type="text" placeholder=" CPF">
                <input id="rg" type="text" placeholder=" RG">
                <input id="cep" type="text" placeholder=" CEP">
                <input id="endereco" type="text" placeholder=" Endereço">
                <input id="cidade" type="text" placeholder=" Cidade">
                <input id="numero" type="text" placeholder=" Nº">
                <input id="estado" type="text" placeholder=" Estado">
            </div>
            <input id="enviar" type="submit">
        </main>
    </body>

    </html>